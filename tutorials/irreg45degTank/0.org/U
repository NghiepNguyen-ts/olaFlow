FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            waveVelocity;
        waveDictName    waveDict;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            waveAbsorption3DVelocity;
        nPaddles        25;
        nEdgeMin        1;
        nEdgeMax        1;
        value           uniform (0 0 0);
    }
    bottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    atmosphere
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    front
    {
        type            waveAbsorption3DVelocity;
        nPaddles        25;
        nEdgeMin        0;
        nEdgeMax        1;
        value           uniform (0 0 0);
    }
    back
    {
        type            waveAbsorption3DVelocity;
        nPaddles        25;
        nEdgeMin        0;
        nEdgeMax        1;
        value           uniform (0 0 0);
    }
}


// ************************************************************************* //
